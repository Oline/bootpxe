DISPLAY boot.txt
SERIAL  0 115200
PROMPT  1
TIMEOUT 0

DEFAULT lenny_i386_netinst

label alix2d3
  menu label ^net install lenny
  menu default
  kernel debian-installer/i386/linux
  append initrd=debian-installer/i386/initrd.gz console=ttyS0,38400n8  --

LABEL lenny_i386_netinst
  menu label install Debian Lenny Netinst
  kernel debian/lenny/i386/linux_kernel_debian_netinst.2.6.26-2-486
  append initrd=debian/lenny/i386/linux_kernel_debian_netinst_initrd.2.6.26-2-486.gz console=ttyS0,115200n8
